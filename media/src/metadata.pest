WHITESPACE = _{ " "+ | "\t"+ }

input = { SOI ~ line* ~ EOI }

line = { source ~ protocol ~ ":" ~ key ~ value ~ NEWLINE? }

source   = @{ (ASCII_ALPHANUMERIC | "_")+ }
protocol = @{ (ASCII_ALPHANUMERIC | "_")+ }
key      = @{ (ASCII_ALPHANUMERIC | "_")+ }

value = @{ (!NEWLINE ~ ASCII)* }

NEWLINE = _{ "\n" | "\r\n" }
